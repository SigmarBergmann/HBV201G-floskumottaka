<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" prefHeight="420.0" prefWidth="625.0" spacing="20.0" stylesheets="@css/floskur.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vidmot.floskumottaka.KvedjaController">
    <children>
        <Label fx:id="fxKvedjaText" alignment="CENTER" contentDisplay="CENTER" prefHeight="401.0" prefWidth="620.0" text="Við þökkum fyrir heimsóknina! Sjáumst vonandi fljótlega aftur!" />
        <ButtonBar prefHeight="36.0" prefWidth="625.0" styleClass="button-bar">
            <buttons>
                <Button fx:id="KvedjaTakki" mnemonicParsing="false" onAction="#fxKvedjaTakki" text="Hætta" />
            </buttons>
        </ButtonBar>
    </children>
</VBox>


